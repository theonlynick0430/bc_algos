{
    "experiment": {
        "name": "bc_transformer",
        "ouput_dir": null,
        "valid_rate": 1,
        "save_rate": 100, 
        "rollout_rate": 100
    },

    "train": {
        "epochs": 1000,
        "batch_size": 16, 
        "learning_rate": 0.0005,
        "loss": "L2"
    },

    "observation": {
        "shapes": {
            "robot0_eef_pos": [3],
            "robot0_eef_quat": [4],
            "agentview_image": [3, 224, 224]
        },
        "modalities": {
            "obs": {
                "low_dim": [
                    "robot0_eef_pos",
                    "robot0_eef_quat"
                ],
                "rgb": ["agentview_image"]
            },
            "goal": {
                "low_dim": [],
                "rgb": ["agentview_image"]
            }
        },
        "kwargs": {
            "low_dim": {
                "output_shape": [9],
                "hidden_dims": [32]
            },
            "rgb": {}
        }
    },

    "dataset": {
        "type": "robomimic",
        "path": "/Users/nikhilsridhar/Desktop/RoboDev/JJ/bc_algos/datasets/test/square_ph.hdf5",
        "dataset_keys": [
            "actions"
        ],
        "frame_stack": 9,
        "seq_length": 10,
        "pad_frame_stack": true,
        "pad_seq_length": true,
        "get_pad_mask": false,
        "goal_mode": null,
        "num_subgoal": null,
        "kwargs": {
            "train": {
                "filter_by_attribute": "train",
                "demos": null
            },
            "valid": {
                "filter_by_attribute": "valid",
                "demos": null
            }
        }
    },

    "policy": {
        "type": "transformer",
        "action_shape": [7],
        "kwargs": {
            "backbone": {
                "nlayers": 6,
                "nhead": 6, 
                "pos_embed_kwargs": {},
                "enc_kwargs": {}
            },
            "action_decoder": {
                "hidden_dims": [32]
            }
        }
    },

    "rollout": {
        "type": "robomimic",
        "horizon": null,
        "render_video": false
    }
}