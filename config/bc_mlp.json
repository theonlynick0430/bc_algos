{
    "experiment": {
        "name": "bc_mlp",
        "ouput_dir": null,
        "valid_rate": 1,
        "save_rate": 50, 
        "rollout_rate": 10
    },

    "train": {
        "epochs": 100,
        "batch_size": 128, 
        "loss": "L2",
        "discount": 1.0,
        "lr": 1e-3,
        "weight_decay": 1e-2,
        "betas": [0.9, 0.95],
        "weights": null
    },

    "observation": {
        "shapes": {
            "robot0_eef_pos": [3],
            "robot0_eef_quat": [4],
            "agentview_image": [3, 224, 224]
        },
        "modalities": {
            "obs": {
                "low_dim": [
                    "robot0_eef_pos",
                    "robot0_eef_quat"
                ],
                "rgb": ["agentview_image"]
            },
            "goal": {
                "low_dim": [],
                "rgb": []
            }
        },
        "kwargs": {
            "low_dim": {
                "output_shape": [64],
                "hidden_dims": []
            },
            "rgb": {}
        }
    },

    "dataset": {
        "type": "robomimic",
        "path": null,
        "action_key": "actions",
        "frame_stack": 0,
        "seq_length": 1,
        "pad_frame_stack": true,
        "pad_seq_length": true,
        "get_pad_mask": true,
        "goal_mode": null,
        "num_subgoal": null,
        "kwargs": {
            "train": {
                "filter_by_attribute": "train",
                "demos": null
            },
            "valid": {
                "filter_by_attribute": "valid",
                "demos": null
            }
        }
    },

    "policy": {
        "type": "mlp",
        "action_shape": [7],
        "kwargs": {
            "backbone": {
                "output_dim": 64,
                "hidden_dims": [256, 128]
            },
            "action_decoder": {}
        }
    },

    "rollout": {
        "type": "robomimic",
        "ortho6D": false,
        "world": false,
        "env_cfg_path": null,
        "closed_loop": true,
        "render_video": false,
        "video_skip": 1,
        "terminate_on_success": false,
        "horizon": null,
        "verbose": false
    }
}